    "TARGET_FOLDERS = [\"dataset/video1\", \"dataset/video2\", \"dataset/video3\"]\n",
    "OUTPUT_DIR = \"output\"\n",
    "os.makedirs(OUTPUT_DIR, exist_ok=True)\n",
    "\n",
    "# 처리할 파일 확장자\n",
    "AUDIO_EXTENSIONS = [\".mp4\", \".mov\", \".mkv\", \".avi\", \".m4a\", \".mp3\", \".wav\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
